             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BeginnersTasks.SettingsPage">

    <VerticalStackLayout
        Padding="20"
        Spacing="10">

        <Button Text="Set CurrentItem"
                Clicked="SetCurrentItem_Clicked"
                HorizontalOptions="Center"
                VerticalOptions="Center"/>

        <Button Text="GoToAsync"
                Clicked="GoToAsync_Clicked"
                HorizontalOptions="Center"
                VerticalOptions="Center"/>
    </VerticalStackLayout>
</ContentPage>